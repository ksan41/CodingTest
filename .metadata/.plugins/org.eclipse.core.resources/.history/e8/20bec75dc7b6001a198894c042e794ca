package test.exam;

public class main {

	public static void main(String[] args) {
		
		Solution s = new Solution();
		
		//System.out.println(s.solution("zAZ", 4));
		System.out.println(s.solution(12));
	}

}

class Solution {
    public boolean solution(int x) {
        boolean answer = false;
        
        int h = x/10 + x%10;
        if(x%h == 0){
            answer = true;
        }
        
        return answer;
    }
}




