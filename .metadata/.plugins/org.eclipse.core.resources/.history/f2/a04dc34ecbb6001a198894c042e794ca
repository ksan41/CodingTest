package test.exam;

public class main {

	public static void main(String[] args) {
		
		Solution s = new Solution();
		
		//System.out.println(s.solution("zAZ", 4));
		System.out.println(s.solution(1800));
	}

}

class Solution {
    public boolean solution(int x) {
        boolean answer = true;
        
        if(x>=10){
         if(x%10!=0) {
        	 System.out.println("?");
        	 int h = x/10000 + x/1000+ x/100 + x%10;   
        	 if(x%h == 0){
        		 return answer;
        	 }else {
        		 answer = false;
        	 }
         }else {
        	 System.out.println("이쪽이다");
        	 int h = x/10000 + x/1000+ x/10;
        	 if(x%h == 0){
        		 return answer;
        	 }else {
        		 answer = false;
        	 }
         }
        }
        
        
        
        return answer;
    }
}



